{
    "name": "krzysztofmazur/php-object-mapper",
    "description": "Object mapper for PHP, convert your Model to DTO objects",
    "license": "MIT",
    "type": "library",
    "homepage": "https://github.com/krzysztofmazur",
    "authors": [
        {"name": "Krzysztof Mazur", "email": "krz@ychu.pl"}
    ],
    "require": {
        "php": ">=5.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "doctrine/instantiator": "^1.0"
    },
    "suggest": {
        "doctrine/instantiator": "For creating new instances of objects (you can provide own implementation)"
    },
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
